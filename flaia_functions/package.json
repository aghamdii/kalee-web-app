{
  "name": "flaia-ai-functions",
  "version": "1.0.0",
  "description": "Cloud Functions for Flaia AI itinerary generation",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "22"
  },
  "dependencies": {
    "@google/genai": "^1.5.0",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^6.0.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^24.0.1",
    "typescript": "^5.8.3"
  },
  "private": true
}